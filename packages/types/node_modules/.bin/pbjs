#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/amigo/Develop/stafi/stafihub-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/amigo/Develop/stafi/stafihub-app/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/protobufjs@6.11.2/node_modules/protobufjs/bin/pbjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/protobufjs@6.11.2/node_modules/protobufjs/bin/pbjs" "$@"
fi
