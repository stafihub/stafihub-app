{
  "name": "@stafihub/apps-config",
  "version": "4.0.1",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stafihub/stafihub-app.git"
  },
  "keywords": [
    "stafihub",
    "cosmos"
  ],
  "author": "joe",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.17.3",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/lodash": "4.14.180",
    "@types/webpack-env": "1.16.3"
  },
  "eslintConfig": {
    "extends": []
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "compile": "npx babel --copy-files --extensions .ts,.tsx src -d dist",
    "publish-lib": "npx tsc && pnpm publish --no-git-checks",
    "publish-lib-babel": "npx babel --extensions .ts,.tsx src -d dist --copy-files && npx tsc --emitDeclarationOnly && pnpm publish --no-git-checks"
  }
}
